@model Piece

<h3>@Model.Name Information</h3>

<table class="table">
    <tr>
        <th>Piece ID</th>
        <th>Name</th>
        <th>Price</th>
        <th>Image</th>
        <th>Artist</th>
        <th>Description</th>
    </tr>
    <tr>
        <td>@Model.PieceID</td>
        <td>@Model.Name</td>
        <td>@Model.Price</td>
        <td>@Model.Image</td>
        <td>@Model.Artist</td>
        <td>@Model.Description</td>
    </tr>
</table>